<form>
  <label>Security Dashboard</label>
  <fieldset submitButton="false" autoRun="true">
    <input type="dropdown" searchWhenChanged="true" token="host">
      <label>Netscaler</label>
      <default>*</default>
      <search>
        <query>`nsindex` eventtype=ns_appfw | dedup host | sort host | table host</query>
      </search>
      <choice value="*">all</choice>
      <fieldForLabel>host</fieldForLabel>
      <fieldForValue>host</fieldForValue>
      <populatingSearch fieldForLabel="host" fieldForValue="host">`nsindex` eventtype=ns_appfw | dedup host | sort host | table host</populatingSearch>
    </input>
    <input type="dropdown" searchWhenChanged="true" token="vhost">
      <label>Select vHost:</label>
      <default>*</default>
      <search>
        <query>`nsindex` eventtype=ns_appfw host=$host$ | top vhost</query>
      </search>
      <choice value="*">all</choice>
      <fieldForLabel>vhost</fieldForLabel>
      <fieldForValue>vhost</fieldForValue>
      <populatingSearch fieldForLabel="vhost" fieldForValue="vhost">`nsindex` eventtype=ns_appfw host=$host$ | top vhost</populatingSearch>
    </input>
    <input type="dropdown" searchWhenChanged="true" token="Country">
      <label>Select Country:</label>
      <default>*</default>
      <search>
        <query>`nsindex` eventtype=ns_appfw host=$host$ vhost=$vhost$ | iplocation src | top Country | table Country</query>
      </search>
      <choice value="*">all</choice>
      <fieldForLabel>Country</fieldForLabel>
      <fieldForValue>Country</fieldForValue>
      <initialValue>*</initialValue>
      <populatingSearch fieldForLabel="Country" fieldForValue="Country">`nsindex` eventtype=ns_appfw host=$host$ vhost=$vhost$ | iplocation src | top Country | table Country</populatingSearch>
    </input>
    <input type="dropdown" searchWhenChanged="true" token="src">
      <label>Select Client IP:</label>
      <default>*</default>
      <search>
        <query>`nsindex` eventtype=ns_appfw host=$host$ vhost=$vhost$ | top limit=100 src | table src</query>
      </search>
      <choice value="*">all</choice>
      <fieldForLabel>src</fieldForLabel>
      <fieldForValue>src</fieldForValue>
      <populatingSearch fieldForLabel="src" fieldForValue="src">`nsindex` eventtype=ns_appfw host=$host$ vhost=$vhost$ | top limit=100 src | table src</populatingSearch>
    </input>
    <input type="dropdown" searchWhenChanged="true" token="act">
      <label>Select action:</label>
      <default>*</default>
      <search>
        <query>`nsindex` eventtype=ns_appfw | dedup act | table act</query>
      </search>
      <choice value="*">all</choice>
      <fieldForLabel>act</fieldForLabel>
      <fieldForValue>act</fieldForValue>
      <populatingSearch fieldForLabel="act" fieldForValue="act">`nsindex` eventtype=ns_appfw | dedup act | table act</populatingSearch>
    </input>
    <input type="time" searchWhenChanged="true">
      <label>Select Time:</label>
      <default>
        <earliestTime>-4h@m</earliestTime>
        <latestTime>now</latestTime>
      </default>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>Security Metrics</title>
      <single>
	    <searchString>`nsindex` eventtype=ns_appfw host=$host$ vhost=$vhost$ src=$src$ act="$act$"   | iplocation src | search Country="$Country$" | stats count</searchString>
        <search>
          <query>`nsindex` eventtype=ns_appfw host=$host$ vhost=$vhost$ src=$src$ act="$act$"   | iplocation src | search Country="$Country$" | stats count</query>
        </search>
        <option name="underLabel">Attacks</option>
        <option name="linkView">search</option>
        <option name="drilldown">none</option>
        <option name="colorBy">value</option>
        <option name="colorMode">none</option>
        <option name="numberPrecision">0</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="useColors">0</option>
        <option name="useThousandSeparators">1</option>
      </single>
      <single>
        <searchString>`nsindex` eventtype=ns_appfw host=$host$ vhost=$vhost$ src=$src$ act="$act$"   | iplocation src | search Country="$Country$"   | stats dc(Country) as Country</searchString>
